<NinePlaces:TickElement
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
    xmlns:NinePlaces="clr-namespace:NinePlaces"
	x:Class="NinePlaces.TimeZoneSegment"
	d:DesignWidth="40" d:DesignHeight="40">


    <Grid x:Name="LayoutRoot" Background="Blue" Height="Auto" Width="Auto" UseLayoutRounding="False" VerticalAlignment="Stretch" ShowGridLines="False">
        <Canvas x:Name="TickCanvas" Height="Auto" Width="Auto" Background="#00333333">
            <Path x:Name="Tick" Height="10" Margin="0,0,0,0" VerticalAlignment="Top" Fill="{x:Null}" Stretch="Fill" Stroke="#FF808080" StrokeThickness="1" Data="M8.333333,11.666666 L8.333333,26.401592" Width="1" HorizontalAlignment="Left"/>
            <TextBlock x:Name="DateText" Margin="3,3,3,3" VerticalAlignment="Bottom" Visibility="Collapsed"></TextBlock>
        </Canvas>
    </Grid>

</NinePlaces:TickElement>

