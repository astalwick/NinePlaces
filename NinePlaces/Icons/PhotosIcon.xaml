<NinePlaces:IconControlBase x:Class="NinePlaces.PhotosIcon"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:NinePlaces="clr-namespace:NinePlaces"
    xmlns:vsm="clr-namespace:System.Windows;assembly=System.Windows"             
    Width="400" Height="300">


<Canvas x:Name="StdBaseCanvas" Width="Auto" Height="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <vsm:VisualStateManager.VisualStateGroups>
            <vsm:VisualStateGroup x:Name="VSVisibility">
                <vsm:VisualStateGroup.Transitions>
                    <vsm:VisualTransition GeneratedDuration="00:00:00.2500000"/>
                    <vsm:VisualTransition GeneratedDuration="00:00:01.5" To="NotVisible"/>
                </vsm:VisualStateGroup.Transitions>
                <vsm:VisualState x:Name="VSNotVisible">
                    <Storyboard>
                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0010000" Storyboard.TargetName="StdBaseGrid" Storyboard.TargetProperty="(UIElement.Opacity)">
                            <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                        </DoubleAnimationUsingKeyFrames>
                    </Storyboard>
                </vsm:VisualState>
                <vsm:VisualState x:Name="VSVisible"/>
            </vsm:VisualStateGroup>
        </vsm:VisualStateManager.VisualStateGroups>
    	<Grid x:Name="StdBaseGrid" Width="Auto" Height="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
    		<Canvas>
    			<!-- blue/<Path> -->
    			<Rectangle x:Name="StdBGRect" Width="32" Height="32" Fill="{StaticResource MemoriesIconColor}" RadiusX="6" RadiusY="6">
    				<Rectangle.RenderTransform>
    					<TransformGroup>
    						<ScaleTransform x:Name="BGScale" ScaleX="1.00"/>
    					</TransformGroup>
    				</Rectangle.RenderTransform>
    			</Rectangle>
    			<Rectangle Width="32" Height="32" Fill="{StaticResource MemoriesIconGradient}" RadiusX="6" RadiusY="6" >
    				<Rectangle.RenderTransform>
    					<TransformGroup>
    						<ScaleTransform x:Name="BGOpacityScale" ScaleX="1.00"/>
    					</TransformGroup>
    				</Rectangle.RenderTransform>
    			</Rectangle>

			<Grid>
				<Grid.RenderTransform>
    					<TransformGroup>
    						<ScaleTransform x:Name="IconScale" ScaleX="1.00"/>
    					</TransformGroup>				
				</Grid.RenderTransform>

				  <!-- Layer 1/<Path> -->
				  <Path Fill="#ffffffff" Data="F1 M 28.642,6.871 C 28.642,6.164 27.972,5.589 27.147,5.589 L 8.234,5.381 C 7.311,5.584 6.341,6.413 5.538,6.987 L 24.165,7.464 C 25.060,7.464 25.786,8.087 25.786,8.854 L 26.198,24.188 C 27.478,23.195 28.825,22.359 29.021,21.168 L 28.642,6.871 Z"/>

