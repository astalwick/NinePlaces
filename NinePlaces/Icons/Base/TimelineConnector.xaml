<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="NinePlaces.TimelineConnector"
	d:DesignWidth="2" d:DesignHeight="90">

	<Grid x:Name="LayoutRoot">
		<Path x:Name="ConnectorPath" Stretch="UniformToFill" Stroke="#FFB9B9B9" StrokeDashArray="4 4" StrokeThickness="1.0" Data="M140,341 L140,405" Visibility="Visible"/>
		<Canvas VerticalAlignment="Bottom" x:Name="DurationCanvas" Height="10">
			<Rectangle Visibility="Collapsed" x:Name="DurationIndicator" VerticalAlignment="Bottom" Height="10" Width="100" Fill="#FFFFFFFF" Stroke="#FF000000"/>
		</Canvas>
	</Grid>
</UserControl>